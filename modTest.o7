

MODULE modTest;;

IMPORT mT:=modTest;


VAR
   a:INTEGER;

PROCEDURE Test*(a_:INTEGER):BOOLEAN;
   VAR 
      res : BOOLEAN;
      lt : ARRAY 256 OF CHAR
   BEGIN
      res:=(a_>a);
      lt := "MODULE";
      IF ( res >= a) THEN
         res := -1
      END;
      RETURN res
   END Test;

BEGIN
   a:=10
END modTest.

